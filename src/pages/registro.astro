---
import Layout from "../layouts/Layout.astro";
import RegistrationForm from "../components/Registro/RegistrationForm.jsx";
---

<Layout title="Digeek 2024">
  <main
    class="w-full pb-10 mt-[100px] md:py-0 md:h-[calc(100vh-100px)] flex flex-col md:flex-row justify-center items-center"
  >
    <RegistrationForm client:load />
    <img
      src="/Logo.png"
      class="w-[200px] md:w-[500px] h-fit object-cover order-first md:-order-first"
    />
  </main>
</Layout>
